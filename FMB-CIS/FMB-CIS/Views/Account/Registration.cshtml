@model FMB_CIS.Models.UserRegistrationViewModel
@* @model FMB_CIS.Models.UserRegistrationViewModel *@

@{
    ViewBag.Title = "Registration";
    var baseurl = ViewBag.BaseUrl;
}

<link rel="stylesheet" href="/font-awesome/css/all.css" />

<link href="/bootstrap-fileinput-master/css/fileinput.min.css" media="all" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.0/font/bootstrap-icons.css" />

<div class="container-fluid d-flex flex-column align-items-center justify-content-center">
    <div class="col-md-12 p-md-2 text-start">
        <img src="~/assets/images/logo.png" height="100" />
    </div>
    <hr class="hr" />

    <div class="container"></div><div class="container">
        <a href="@Url.Action("Index", "Home")" class="me-auto my-4" title="back to login"><i data-feather="arrow-left" class="text-secondary backBtn"></i></a>
        <h3 class="text-secondary fw-bold my-4">REGISTER YOUR ACCOUNT</h3>
        <div class="stepwizard col-md-offset-3">
            <div class="stepwizard-row setup-panel">
                <div class="stepwizard-step">
                    <p class="mb-1 fw-bold">Step</p>
                    <a href="#step-1" type="button" class="btn btn-primary btn-circle fw-bold">1</a>
                    <br />
                    Choose User Type
                </div>
                <div class="stepwizard-step">
                    <p class="mb-1 fw-bold">Step</p>
                    <a href="#step-2" type="button" class="btn btn-default btn-circle fw-bold" disabled="disabled">2</a> <br />
                    Fill Out User Information
                </div>
                <div class="stepwizard-step">
                    <p class="mb-1 fw-bold">Step</p>
                    <a href="#step-3" type="button" class="btn btn-default btn-circle fw-bold" disabled="disabled">3</a>
                    <br />
                    Upload Supporting Documents
                </div>
                @* <div class="stepwizard-step">
                <p class="mb-1 fw-bold">Step</p>
                <a href="#step-4" type="button" class="btn btn-default btn-circle fw-bold" disabled="disabled">4</a>
                </div>
                <div class="stepwizard-step">
                <p class="mb-1 fw-bold">Step</p>
                <a href="#step-5" type="button" class="btn btn-default btn-circle fw-bold" disabled="disabled">5</a>
                </div>
                <div class="stepwizard-step">
                <p class="mb-1 fw-bold">Step</p>
                <a href="#step-6" type="button" class="btn btn-default btn-circle fw-bold" disabled="disabled">6</a>
                </div> *@
            </div>
        </div>

        <form asp-action="Registration" id="RegForm" role="form" class="w-75 mx-auto mb-5" method="post" enctype="multipart/form-data">

            <div class="row setup-content border rounded pt-3 pb-4" id="step-1">
                <label class="control-label text-secondary fw-bold mb-3">Choose your user type & classification</label>
                <div class="col-xs-6 col-md-offset-3">
                    <div class="col-md-12">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label text-secondary small mb-1">User Type</label>
                                    <input type="hidden" asp-for="tbl_user_types_string" id="tbl_user_types_ids" />
                                    @*  <select asp-for="tbl_Users.tbl_user_types_id" class="form-select" aria-label="User Type" required="required">
                                    <option value="" selected disabled>--Select User Type--</option>
                                    <option value="1">Chainsaw Importer Only</option>
                                    <option value="2">Chainsaw Seller Only</option>
                                    <option value="3">Chainsaw Owner Only</option>
                                    <option value="4">Chainsaw Importer and Seller</option>
                                    <option value="5">Chainsaw Importer and Owner</option>
                                    <option value="6">Chainsaw Seller and Owner</option>
                                    <option value="7">Chainsaw Importer, Owner, and Seller</option>
                                    </select>*@
                                    <label for="checkbox_chainsawImporter" class="form-check form-check-border">
                                        <input class="form-check-input user_types_button" type="checkbox" value="1" id="checkbox_chainsawImporter">
                                        @*name="tbl_Users.tbl_user_types_ids"*@
                                        <label class="form-check-label" for="checkbox_chainsawImporter">
                                            Chainsaw Importer
                                        </label>
                                    </label>
                                    <label for="checkbox_chainsawSeller" class="form-check form-check-border">
                                        <input class="form-check-input user_types_button" type="checkbox" value="2" id="checkbox_chainsawSeller">
                                        @*name="tbl_Users.tbl_user_types_ids"*@
                                        <label class="form-check-label" for="checkbox_chainsawSeller">
                                            Chainsaw Seller
                                        </label>
                                    </label>
                                    <label for="checkbox_chainsawOwner" class="form-check form-check-border">
                                        <input class="form-check-input user_types_button" type="checkbox" value="3" id="checkbox_chainsawOwner">
                                        @*name="tbl_Users.tbl_user_types_ids"*@
                                        <label class="form-check-label" for="checkbox_chainsawOwner">
                                            Chainsaw Owner
                                        </label>
                                    </label>
                                    <span asp-validation-for="tbl_user_types_ids" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label text-secondary small mb-1">User Classification</label>
                                    <select asp-for="tbl_Users.user_classification" class="form-select" aria-label="User Classification" onchange="userClassification(this)" required>
                                        <option value="" selected disabled>--Select User Classification--</option>
                                        <option value="Individual">Individual</option>
                                        <option value="Corporate/Cooperative">Corporate/Cooperative</option>
                                        <option value="Government">Government</option>
                                    </select>
                                    <span asp-validation-for="tbl_Users.user_classification" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="text-center mt-3">
                                <button class="btn btn-primary nextBtn text-center" type="button">Next</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row setup-content border rounded pt-3 pb-4" id="step-2">
                <label class="control-label text-secondary fw-bold mb-3">Fill out your information</label>
                <div class="col-xs-6 col-md-offset-3">
                    <div class="col-md-12">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="row mb-4">
                            <div class="col-md-10">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label asp-for="tbl_Users.first_name" class="control-label small text-secondary mb-1">First Name</label>
                                            <input asp-for="tbl_Users.first_name" class="form-control" required="required" placeholder="Enter First Name" />
                                            <span asp-validation-for="tbl_Users.first_name" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label asp-for="tbl_Users.middle_name" class="control-label small text-secondary mb-1">Middle Name</label>
                                            <input asp-for="tbl_Users.middle_name" class="form-control" required="required" placeholder="Enter Middle Name" />
                                            <span asp-validation-for="tbl_Users.middle_name" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label asp-for="tbl_Users.last_name" class="control-label small text-secondary mb-1">Last Name</label>
                                            <input asp-for="tbl_Users.last_name" class="form-control" required="required" placeholder="Enter Last Name" />
                                            <span asp-validation-for="tbl_Users.last_name" class="text-danger"></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label asp-for="tbl_Users.suffix" class="form-label small mb-1">Suffix (If any)</label>
                                    <input asp-for="tbl_Users.suffix" class="form-control" placeholder="Enter Suffix" />
                                    <span asp-validation-for="tbl_Users.suffix" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <div class="row mb-4">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label asp-for="tbl_Users.email" class="control-label small text-secondary mb-1">Email (Please use official email address)</label>
                                    <input asp-for="tbl_Users.email" class="form-control" placeholder="name@address.com" required="required" id="emailInput" onchange="checkExistingEmail()" />
                                    <span asp-validation-for="tbl_Users.email" class="text-danger"></span>
                                    <span id="emailValidation" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label asp-for="tbl_Users.contact_no" class="control-label small text-secondary mb-1">Contact Number (11 Digits)</label>
                                    <input asp-for="tbl_Users.contact_no" type="tel" pattern="[0-9]{11}" class="form-control" placeholder="09XX XXX XXXX" required="required" />
                                    <span asp-validation-for="tbl_Users.contact_no" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <div class="row mb-4">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="control-label small text-secondary mb-1">Gender</label>
                                    <select asp-for="tbl_Users.gender" class="form-select" aria-label="Gender" required>
                                        <option value="" selected disabled>--Select Gender--</option>
                                        <option value="Male">Male</option>
                                        <option value="Female">Female</option>
                                    </select>
                                    <span asp-validation-for="tbl_Users.gender" class="text-danger"></span>
                                </div>
                            </div>
