                                        <option value="SSS ID">SSS ID</option>
                                        <option value="Other valid government-issued IDs">Other valid government-issued IDs</option>
                                    </select>
                                    <span asp-validation-for="tbl_Users.valid_id" class="text-danger"></span>
                                </div>
