@model FMB_CIS.Models.ViewModel

<link rel="stylesheet" href="~/js/DataTable/dataTables.min.css">
<script src="~/js/jquery/jquery-3.7.1/jquery-3.7.1.js"></script>
<script src="~/js/DataTable/dataTables.min.js"></script>

<div id="card">
    <h1>
        Imported Chainsaws
    </h1>
    <p>
        <table class="table dataTable" id="tblImportedChainsaws">
            <thead>
                <tr>
                    <th>
                        Engine
                    </th>
                    <th>
                        Brand
                    </th>
                    <th>
                        Model
                    </th>
                    <th>
                        Power
                    </th>

                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.ImportedChainsaws)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.engine_serialNo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.brand)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.model)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.power_source)
                        </td>
                    </tr>                    
                }
            </tbody>
        </table>
    </p>
</div>
